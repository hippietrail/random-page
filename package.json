{
    "activationEvents": [
        "onCommand:extension.randomFile"
    ],
    "configuration": {
        "title": "Open a random file",
        "properties": {
            "randomFile.extensions": {
                "type": "array",
                "default": [
                    ".htm", ".html",
                    ".md", ".txt",
                    ".asm", ".s",
                    ".c", ".cpp", ".h",
                    ".cs",
                    ".dart",
                    ".go",
                    ".jai",
                    ".java",
                    ".js", ".jsx", ".ts", ".tsx",
                    ".kt",
                    ".lua",
                    ".odin",
                    ".php",
                    ".pl",
                    ".py",
                    ".rb",
                    ".rs",
                    ".sh",
                    ".swift",
                    ".zig"
                ],
                "description": "Only choose files with these extensions"
            }
        }
    },
    "contributes": {
        "commands": [
            {
                "command": "extension.randomFile",
                "title": "Open a random File"
            }
        ]
    },
    "description": "Open a random file from the current workspace",
    "displayName": "Random File",
    "engines": {
        "vscode": "^1.7.5"
    },
    "main": "./out/extension.js",
    "name": "random-file",
    "publisher": "hippietrail",
    "repository": {
        "type": "git",
        "url": "https://github.com/hippietrail/random-page.git"
    },
    "version": "0.1.0"
}
